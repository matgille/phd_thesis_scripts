@article{clavert_les_2019,
 abstract = {Les historiens ne sont pas en reste pour s\&\#8217;investir dans les humanit\&\#233;s num\&\#233;riques et si d\&\#8217;autres disciplines \&\#224; l\&\#8217;instar des sciences de l\&\#8217;information et de la commu\&\#173;nication ({SIC}) ont pu mener un effort collectif pour penser la relation \&\#224; leur discipline, la r\&\#233;flexion his\&\#173;torienne se saisit aussi de ces enjeux. Il est clair que non seulement le num\&\#233;rique et, dans le cas qui nous int\&\#233;resse plus sp\&\#233;cifiquement, les {DH} concernent les historiens \&\#224; tous les stades de leur recherche, du rapport \&\#224; l\&\#8217;archive et de la constitution des corpus \&\#224; la diffusion de leurs travaux, mais ils modifient aussi les espaces de dialogue et de circulation des id\&\#233;es d\&\#8217;une partie de la communaut\&\#233; historienne. En cela les {DH} sont bien un enjeu pour la fabrique et l\&\#8217;\&\#233;criture de l\&\#8217;histoire, mais peuvent aussi \&\#224; terme devenir un objet d\&\#8217;histoire. Si dans cette relation entre {DH} et histoire nous nous int\&\#233;resserons plus modestement \&\#224; ce que fait l\&\#8217;histoire aux {DH}, bien qu\&\#8217;il y ait mati\&\#232;re \&\#224; ana\&\#173;lyser les \&\#233;changes qui se nouent au sein de cette inter\&\#173;discipline et le r\&\#244;le qu\&\#8217;y tiennent ou n\&\#8217;y tiennent pas les historiens, c\&\#8217;est surtout sous l\&\#8217;angle des apports des {DH} \&\#224; l\&\#8217;histoire que nous penserons une int\&\#233;gra\&\#173;tion et une hybridit\&\#233; \&\#224; la fois (inter) et (intra) discipli\&\#173;naire, au service non pas de la dilution disciplinaire mais de son enrichissement.},
 author = {Clavert, Frédéric and Schafer, Valérie},
 date = {2019-03-18},
 issn = {0987-1381},
 journaltitle = {Quaderni},
 langid = {french},
 number = {1},
 pages = {33--49},
 title = {‪Les humanités numériques, un enjeu historique‪},
 url = {https://www.cairn.info/revue-quaderni-2019-1-page-33.html},
 volume = {n° 98}
}

