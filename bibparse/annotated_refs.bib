@article{sanchez-prieto_borja_puntuacion_2017,
 abstract = {El objetivo de este estudio es examinar el uso de los signos de puntuación (calderón, punto, punto y coma) en los códices regios de la General estoria, correspondientes a las partes Primera y Cuarta. Para ello hemos seguido un método tanto cuantitativo como cualitativo sobre los 100 primeros folios de cada manuscrito. Es posible así establecer la frecuencia relativa del empleo de cada signo y determinar los contextos en los que este aparece, con vistas a precisar si la puntuación tiene una función preferentemente sintáctica o son otros los valores que cabe atribuirle en estos códices. Es fundamental plantearse la relación entre puntuación y sintaxis, y en particular si los diferentes signos empleados cortan o no la progresión del discurso. Los resultados al respecto han sido sorprendentes. Hemos comparado también segmentos de diferente fuente y configuración sintáctica para determinar si la variada tipología textual interna de la General estoria influye en el empleo de los signos de puntuación.},
 author = {Sánchez-Prieto Borja, Pedro},
 date = {2017-06-30},
 doi = {10.4000/atalaya.2570},
 issn = {2102-5614},
 journaltitle = {Atalaya},
 langid = {spanish},
 number = {17},
 rights = {Atalaya est mis à disposition selon les termes de la licence Creative Commons Attribution - Pas d'Utilisation Commerciale - Pas de Modification 4.0 International.},
 title = {La puntuación en los códices de la General Estoria de Alfonso X el Sabio},
 url = {http://journals.openedition.org/atalaya/2570}
