@article{fargette_rumeurs_2007,
 abstract = {Between 1407 and 1420, the Armagnacs and Burgundians fought a ruthless war, and their men at arms devastated the \&\#206;le-de-France. Yet, the abominable crimes usually attributed to them (arson, pillage, murder, rape...) are mostly unrecorded. This discrepancy, a major historical problem, can be understood by the spread of rumors and the immoderate use of propaganda. Indeed on both sides it proved essential to manipulate public opinion through manifestos, ballads or sermons, just as it became necessary to keep individuals under observation and control, for every stranger could be a spy or a propagandist. Rumor fed public fears, and one finds village communities taking the initiative and defending themselves against the misdeeds of men at arms. Banditry, far from being the actions of ruffians on the prowl, was in fact presented, in judicial sources, as an act of self-defense.},
 author = {Fargette, Séverine},
 date = {2007-08-01},
 issn = {0027-2841},
 journaltitle = {Le Moyen Age},
 langid = {french},
 number = {2},
 pages = {309--334},
 title = {Rumeurs, propagande et opinion publique au temps de la guerre civile (1407-1420)},
 url = {https://www.cairn.info/revue-le-moyen-age-2007-2-page-309.htm},
 volume = {Tome {CXIII}}
