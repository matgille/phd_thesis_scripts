 langid = {english},
 location = {La Plaine-Saint-Denis},
 pagetotal = {67},
 publisher = {Éditions des maisons des sciences de l'homme associées},
 series = {Collection interdisciplinaire {EMSHA}},
 shorttitle = {The Mosaic Theory of Natural Complexity},
 title = {The Mosaic Theory of Natural Complexity : A scientific and philosophical approach},
 url = {http://books.openedition.org/emsha/200}
}

