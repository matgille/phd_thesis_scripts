@book{carrasco_manchado_discurso_2003,
 abstract = {La presente Tesis Doctoral gira en torno a conceptos tales como el de propaganda o discurso político, que han sido analizados en esta investigación en todas sus implicaciones, ocupándonos con detalle de la historia del acceso al poder de los Reyes Católicos. El estudio del discurso político ha servido para acotar el tema de propaganda política, dada la particular complejidad política del reinado de los Reyes Católicos, y nos hemos ceñido al período inicial de dicho reinado, el momento quizá más apropiado para abrir una investigación como esta, por lo que tiene de crisis de legitimacion, en su doble vertiente, sucesoria y monárquica, y de solución de esta crisis, en lo que se refiere a la cuestión sucesoria, y de ocmienzo de consolidación, en lo que respecta a la superación de la crisis de legitimidad monárquica arrastrada desde antes del siglo {XV}. Hemos logrado detectar en los autores políticos del siglo {XV}, una actitud consciente de emplear, en la lucha política, unos recursos equiparables a lo que hoy se entiende por propaganda política. La existencia, plenamente comprobable, de lo que se ha acuñado como consciencia propagandistica,prueba que es posible estudiar su funcionamiento en el siglo {XV} sin caer en un anacronismo. Se trata de un fenómeno político del que los hombres del siglo {XV} eran conscientes, y, por tanto podrían manejar y manejaban, aunque lógicamente no lo nombraran con los términos actuales, sino que los percibían como técnicas de simulación. Nuestro objetivo ha sido desgranar los acontecimientos ceremoniales y propagandísticos de esa primera etapa del reinado al ritmo de los acontecimientos políticos y obtener así el cuadro ideológico de los principales recursos retóricos y argumentales que sustentaron la propaganda de guerra y de legitimación. Como base de análisis del discurso hemos empleado tanto las fuentes literarias de esa época que surgen en el entorno cortesano y transmiten la propaganda regia de los Reyes Católicos, como fuentes documentales que también la transmiten, en plena consonancia con las necesidades políticas. Los discursos, encuadrados en una tipología básica(jurídico, teológico, ético, discurso de la fama y el honor, de la guerra, del miedo, o del poder), y las estrategias de manipulación lingüística que se observa en su articulación(falacias, estrategias tendentes a representar el consenso, a negar el conflicto, a atraer mediante mensajes sugestivos o a disuadir mediantela retórico de la represión), se construye al hilo de los acontecimientos políticos. Por ello hemos seguido un enfoque estrictamente diacrónico, puesto que la propaganda política está ligada irremediablemente a la coyuntura. Nos hemos ocupado, en una primera parte, del período de la guerra por la sucesión, y en la segunda parte, de la superación de esa crisis de legitimidad, observada desde su vertiente coyuntural y estructural, en tanto que se establecen los pilares de un proceso de construcción del consenso en torno a la idea de sea la monarquía la que regule las relaciones de poder desde una posición incuestionable de soberanía. Se trata, por otra parte, de fenómenos comprobables en todo el contexto Occidental, pudiendo seguirse sus expresiones en el resto de reinos europeos, incluso, en el Papado. Así, a fines del siglo {XV}, y de ello el reinado de los Reyes Católicos sería un ejemplo muy destacado, nos encontraríamos con una correspondencia entre necesidades de índole representativo y medios de representación, constituyendo, por tanto, su análisis, un amplio conjunto de problemas históricos con entidad propia, a los que se ha intentado plantear algunas vías de aproximación.},
 author = {Carrasco Manchado, Ana Isabel and {Universidad Complutense de Madrid} and {Facultad de Geografía e Historia} and {Departamento de Historia Medieval}},
 date = {2003},
 isbn = {978-84-669-1043-9},
 langid = {spanish},
 location = {Madrid},
 note = {{OCLC}: 1025198123},
 publisher = {Universidad Complutense de Madrid, Servicio de Publicaciones},
 title = {Discurso político y propaganda en la Corte de los Reyes Católicos, (1474-1482)}
}

@article{carrasco_manchado_nuevas_2011,
 abstract = {El objetivo de esta investigación es fomentar y ensayar nuevos métodos de análisis de las fuentes medievales, mediante el empleo de los Corpus Textuales Informatizados. Hoy en día, la investigación lingüística en España puede considerarse pionera en el desarrollo de corpus textuales informatizados. Sólo el {CORDE} (Real Academia de la Lengua) permite la búsqueda simultánea de información sobre 12.865 obras y documentos de la época medieval. La historia medieval no puede ignorar el uso de estos recursos para conseguir un mayor apoyo empírico a la interpretación derivada de la formulación de hipótesis de la historia política o social. Nuestro objetivo es informar a la comunidad de medievalistas de los principales corpus textuales informatizados y sus aplicaciones básicas para la investigación histórica.},
