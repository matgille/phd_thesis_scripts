 title = {Philology and the Turn Away from the Linguistic Turn},
 url = {https://www.utpjournals.press/doi/abs/10.3138/flor.32.007},
 volume = {32}
}

@article{ornato_les_1985,
 abstract = {Toute culture qui parvient à un seuil d'élaboration relativement élevé, a besoin, pour se perpétuer, de se matérialiser dans un objet - le codex dans le cas du Moyen Âge occidental - apte à la véhiculer dans les meilleures conditions de fiabilité et d'accessibilité. Cet objet doit être fabriqué, diffusé, lu et conservé. Aussi, les exigences culturelles entrent-elles toujours en conflit avec des contraintes extérieures de tout ordre qui conditionnent, en définitive, la nature profonde et le fonctionnement du processus de transmission. Les deux formes médiévales du codex - le manuscrit et l'imprimé - identiques par leur fonction et très semblables dans leur présentation - baignent en fait dans des systèmes de production et de diffusion radicalement différents : le monde du manuscrit est dominé par la contradiction entre la faible productivité de la copie manuelle et le niveau croissant des besoins culturels ; le monde de l'imprimé est dominé par la contradiction entre la forte productivité du nouveau procédé et le niveau relativement modeste de ces mêmes besoins. L'article cherche à montrer comment ces deux contradictions fondamentales aboutissent, dans un cas, à des pratiques tendant à minimiser les besoins, donc à la stabilité ; dans l'autre, à une organisation concurrentielle et expansionniste dont l'innovation constitue le moteur.},
 author = {Ornato, Ezio},
 copyright = {free},
 journal = {Publications de l'École Française de Rome},
 language = {fre},
 note = {Publisher: Persée - Portail des revues scientifiques en SHS},
 number = {1},
 pages = {57--84},
 title = {Les conditions de production et de diffusion du livre médiéval ({XIIIe}-{XVe} siècles)},
 url = {https://www.persee.fr/doc/efr_0000-0000_1984_act_82_1_2806},
 volume = {82},
 year = {1985}
}

