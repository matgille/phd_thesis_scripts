@incollection{goos_goddag:_2004,
 abstract = {Notations like {SGML} and {XML} represent document structures using tree structures; while this is in general a step forward from earlier systems, it creates certain diﬃculties for the representation of documents in which the structures of interest are not properly nested. Overlapping structures, discontinuous structures, and material which occurs in diﬀerent orders in diﬀerent parts, views, or versions of a document are all problems for {SGML} and {XML}. Overlapping structures have received attention from a variety of authors on {SGML} and {XML}, who have proposed various solutions including the use of non-{SGML} notations with translation into {SGML} for processing, the use of the concur feature of {SGML}, exploitation of conditional marked sections in the {DTD} and document instance, the imposition of various kinds of unusual interpretations on {SGML}/{XML} elements as milestones or as fragments of some larger ‘virtual' element, or the use of detailed annotation separate from the base text being annotated.},
 author = {Sperberg-{McQueen}, C. M. and Huitfeldt, Claus},
