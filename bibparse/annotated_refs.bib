 isbn = {978-2-36279-036-2},
 keywords = {{TROUVER}, History / Europe / Medieval},
 langid = {french},
 pagetotal = {432},
 publisher = {{ALMA} {EDITEUR}},
 title = {Gouverner c'est servir}
}

